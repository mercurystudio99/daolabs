	"lint-staged": {
		"*.{ts,svelte}": "yarn run lint:fix",
		"*.{html,json}": "prettier --write"
	},
	"scripts": {
		"prepare": "husky install",
		"dev": "vite dev --port 8080 --mode development",
		"build": "VITE_LAST_COMMIT_DATA=$(git --no-pager log -1 --format=\"%h %ai\") vite build && node scripts/ipfs-fallback.js",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"test": "npx playwright test",
		"test:unit": "vitest",
		"test:report": "npx playwright test --trace on --reporter=html",
		"test:debug": "npx playwright test --debug",
		"test:pre": "tsc --incremental -p tests/tsconfig.json",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --cache \"src/**/*.{ts,svelte}\"",
		"lint:functions": "eslint --cache functions/src",
		"lint:fix": "eslint --cache --fix",
		"i18n:compile": "lingui compile",
		"ipfs-fallback": "node scripts/ipfs-fallback.js",
		"emulators:start": "firebase emulators:start --export-on-exit=./export-data --import=./export-data",
		"emulators:debug": "firebase emulators:start --export-on-exit=./export-data --import=./export-data --inspect-functions",
		"func:install": "cd functions && npm i",
		"func:build": "cd functions && npm run build",
		"func:deploy": "cd functions && npm run deploy",
		"updateTokens": "node --experimental-fetch scripts/createTokensByAddressFile.js"
	},
